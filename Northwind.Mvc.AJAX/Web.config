    <add key="OData.Northwind" value="http://localhost:65014" />
    <!-- !!! EasyLOB RavenDB -->
    <add key="RavenDB.Northwind.Url" value="http://localhost:8080" />
    <add key="RavenDB.Northwind.Database" value="Northwind" />
    <!-- !!! EasyLOB Redis -->
    <add key="Redis.Northwind" value="localhost" />
